n=string.ascii_letters
m=string.digits
l=['!','"','Â£', '$', '%', '^', '&', '*', '(', ')']


new=[]
p=int(input('Enter length of password you would like '))

for x in range(1):
    f=random.choice(l)
    new.append(f)
for y in range(p-2):
    g=random.choice(n)
    new.append(g)
for z in range(1):
    o=random.choice(m)
    new.append(o)
for q in new:
    print(q, end='')
